  },
  "peerDependencies": {
    "starknet": ">=5.0.0"
  },
  "devDependencies": {
    "@walletconnect/types": "^2.13.0",
    "eslint": "^8.7.0",
    "prettier": "^3.2.5",
    "starknet": "5.24.3",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@walletconnect/sign-client": "^2.13.0",
    "eventemitter3": "^5.0.1",
    "get-starknet-core": "4.0.0-next.2"
  }
}
