{
  "name": "core",
  "private": true,
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --clean true",
    "format:check": "prettier -c \"src/**/*.ts\"",
